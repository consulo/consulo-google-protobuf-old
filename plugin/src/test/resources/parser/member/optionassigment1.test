
option java_package="java.java";
option java_outer_classname="java";
option java_multiple_files=true;
option optimize_for=SPEED;

message a{
option message_set_wire_format = true;
option no_standard_descriptor_accessor=true;
repeated int32 a = 1[default=true];
optional int32 a = 1[deprecated=true];
required int32 a = 1[ctype=STRING];
required int32 a = 1[experimental_map_key="name"];
}


option (idea)= 1;

option idea = 1;
option idea = -1;
option idea = -inf;
option idea = -nan;
option idea = nan;
option idea = inf;
option idea = true;
option idea = idea;
option idea = "idea";
